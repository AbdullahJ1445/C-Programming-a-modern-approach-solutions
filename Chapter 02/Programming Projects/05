#include <stdio.h>

int main()
{
    float x;
    
    printf("Enter a value for x: ");
    scanf("%f", &x);
    
    float polynomial = 3 * (x * x * x * x * x) + 2 * (x * x * x * x) - 5 * (x * x * x) - (x * x) + 7 * (x) - 6;
    
    printf("The value of the polynomial is: %.2f\n", polynomial);
    
    return 0;
}
